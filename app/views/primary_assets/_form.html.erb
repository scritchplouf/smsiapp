<%= simple_form_for(@primary_asset) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :d %>
    <%= f.input :i %>
    <%= f.input :c %>
    <%= f.input :t %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
